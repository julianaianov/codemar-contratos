# Configurações para upload de arquivos grandes
php_value upload_max_filesize 25M
php_value post_max_size 25M
php_value max_execution_time 300
php_value max_input_time 300
php_value memory_limit 256M

# Para Apache 2.4+
<IfModule mod_php8.c>
    php_value upload_max_filesize 25M
    php_value post_max_size 25M
    php_value max_execution_time 300
    php_value max_input_time 300
    php_value memory_limit 256M
</IfModule>
